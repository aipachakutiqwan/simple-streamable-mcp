{
  "mcpServers": {
      
      "filesystem": {
          "command": "npx",
          "args": [
              "-y",
              "@modelcontextprotocol/server-filesystem",
              "."
          ]
      },
      "research": {
          "command": "uv",
          "args": ["run", "src/server/mcp_server.py"]
      },
      "fetch": {
          "command": "uvx",
          "args": ["mcp-server-fetch"]
      }
  }
}
